{
  "name": "csmart-kakaotalk",
  "version": "1.0.0",
  "description": "카카오톡 관리 프로그램 서버",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "test": "vitest",
    "docker:build": "docker build -t csmart-kakaotalk .",
    "docker:run": "docker run -p 3000:3000 csmart-kakaotalk"
  },
  "keywords": ["kakaotalk", "chatbot", "automation", "playwright"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "playwright": "^1.40.0",
    "axios": "^1.6.2",
    "zod": "^3.22.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "@biomejs/biome": "^1.4.1",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
